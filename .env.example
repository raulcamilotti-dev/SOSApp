# N8N Configuration
N8N_URL=http://localhost:5678
N8N_API_KEY=your_api_key_here
N8N_WORKFLOW_ID=Ar17RgJt19MHQwbJqD8ZK

# App Configuration
EXPO_PUBLIC_API_URL=http://localhost:5678/rest/workflows

# ============================================================
# Frontend API Key — used by the Expo app for all API calls
# Same key as the Cloudflare Worker X-Api-Key header
# ============================================================
EXPO_PUBLIC_N8N_API_KEY=

# ============================================================
# API Backend — Cloudflare Worker (default) or N8N webhook (fallback)
# The Worker URL is the default. Set to N8N URL to fall back:
#   EXPO_PUBLIC_API_BASE_URL=https://n8n.sosescritura.com.br/webhook
# ============================================================
EXPO_PUBLIC_API_BASE_URL=https://api-crud.sosescritura.com.br

# Documenso - Document Signing
EXPO_PUBLIC_DOCUMENSO_URL=https://documenso.sosescritura.com.br
EXPO_PUBLIC_DOCUMENSO_API_KEY=

# ============================================================
# Scripts — API key for node scripts (same as EXPO_PUBLIC_N8N_API_KEY)
# ============================================================
SOS_API_KEY=

# ============================================================
# Scripts — Documenso key for ICP-Brasil signing scripts
# ============================================================
DOCUMENSO_API_KEY=
DOCUMENSO_URL=https://documenso.sosescritura.com.br
API_CRUD_ENDPOINT=https://n8n.sosescritura.com.br/webhook/api_crud

# ============================================================
# Scripts — Tenant IDs
# ============================================================
RADUL_TENANT_ID=

# ============================================================
# Database — used by migration/setup scripts
# ============================================================
DB_PASSWORD=

# ============================================================
# Gov.br — Login via Conta Gov.br (OAuth2 / OpenID Connect)
# Registro: https://acesso.gov.br
# Docs: https://manual-roteiro-integracao-login-unico.servicos.gov.br/
# ============================================================
EXPO_PUBLIC_GOVBR_CLIENT_ID=
EXPO_PUBLIC_GOVBR_CLIENT_SECRET=
# "staging" ou "production"
EXPO_PUBLIC_GOVBR_ENV=staging

# ============================================================
# ONR / SREI — Operador Nacional do Registro de Imóveis
# Requer convênio com ONR: https://www.onr.org.br/
# ============================================================
EXPO_PUBLIC_ONR_API_URL=https://api.onr.org.br/srei/v1
EXPO_PUBLIC_ONR_API_KEY=
EXPO_PUBLIC_ONR_CERT_PATH=

# ============================================================
# Plausible Analytics — Analytics open-source, LGPD-friendly
# Self-hosted ou Cloud: https://plausible.io
# ============================================================
EXPO_PUBLIC_PLAUSIBLE_URL=https://analytics.sosescritura.com.br
EXPO_PUBLIC_PLAUSIBLE_DOMAIN=app.sosescritura.com.br
EXPO_PUBLIC_PLAUSIBLE_ENABLED=true
EXPO_PUBLIC_PLAUSIBLE_API_KEY=

# ============================================================
# Metabase — BI & Relatórios
# Self-hosted: https://metabase.sosescritura.com.br
# ============================================================
EXPO_PUBLIC_METABASE_URL=https://bi.sosescritura.com.br
EXPO_PUBLIC_METABASE_API_KEY=

# ============================================================
# ReceitaWS — Consulta CNPJ (pago: token; grátis: 3 req/min)
# https://receitaws.com.br
# ============================================================
EXPO_PUBLIC_RECEITAWS_TOKEN=
