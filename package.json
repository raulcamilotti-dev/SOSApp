{
  "name": "sos-escritura",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "build": "expo export --platform web && node scripts/fix-web-fonts.js",
    "generate:crud": "node scripts/generate-crud.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "sync:n8n:upload": "node scripts/sync-n8n-workflow.js upload",
